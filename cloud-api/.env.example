# QuoTrading Cloud API - Environment Variables Template
# Copy this to .env and fill in your values

# PostgreSQL Database (Render provides this automatically)
DATABASE_URL=postgresql://user:password@host:5432/quotrading

# Stripe API Keys (Get from https://dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# API Security
API_SECRET_KEY=change-this-to-random-secret-in-production

# App Settings
DEBUG=False
